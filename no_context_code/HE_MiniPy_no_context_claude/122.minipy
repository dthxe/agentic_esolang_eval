def add_elements(arr, k):
    result = 0
    count = 0
    
    i = 0
    while i < len(arr) and count < k:
        # Check if the element has at most two digits
        if 0 <= arr[i] <= 99:
            result = result + arr[i]
        
        count = count + 1
        i = i + 1
    
    return result
