def fizz_buzz(n):
    count = 0
    i = 0
    while i < n:
        if i % 11 == 0 or i % 13 == 0:
            # Count occurrences of 7 in the number
            num_str = str(i)
            j = 0
            while j < len(num_str):
                if num_str[j] == '7':
                    count = count + 1
                j = j + 1
        i = i + 1
    return count
