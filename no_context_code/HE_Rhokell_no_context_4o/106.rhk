# Rhokell code for HumanEval/106
# Task: Create a list based on factorial and sum

# Define a function to implement f
func f(n) {
    result = []
    
    # Calculate values
    for i in range(1, n+1) {
        if i % 2 == 0 {
            result.append(factorial(i))
        } else {
            result.append(sum(range(1, i+1)))
        }
    }
    return result
}
