# Rhokell code for HumanEval/99
# Task: Find the closest integer

# Define a function to find closest integer
func closest_integer(value) {
    num = float(value)
    
    # Calculate closest integer
    if num > 0 {
        return floor(num + 0.5)
    } else {
        return ceil(num - 0.5)
    }
}
